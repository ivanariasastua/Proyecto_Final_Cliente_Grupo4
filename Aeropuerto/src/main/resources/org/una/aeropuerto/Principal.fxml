            <ScrollPane id="menu" fx:id="spMenu" hbarPolicy="NEVER" prefHeight="650.0" prefWidth="300.0">
               <content>
                  <Accordion id="accordioMenu" prefHeight="650.0" prefWidth="286.0">
                     <panes>
                        <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="TOP" expanded="false" prefHeight="0.0" prefWidth="250.0" text="Inicio">
                           <graphic>
                              <ImageView id="imvInicio" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="TOP" expanded="false" text="Empleados">
                           <graphic>
                              <ImageView id="imvEmpleados" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="TOP" expanded="false" text="Areas de Trabajo">
                           <graphic>
                              <ImageView id="imvAreas" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
