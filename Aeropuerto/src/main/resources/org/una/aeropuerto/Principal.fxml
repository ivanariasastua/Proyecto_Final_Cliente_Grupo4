                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" contentDisplay="TOP" layoutX="10.0" layoutY="60.0" text="Gastos de Servicios">
                           <graphic>
                              <ImageView id="imvGastos" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                           <content>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionServicios" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Servicios">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionRegistrarGasto" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Registro de Gastos">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionSeguimientoGasto" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Seguimiento de Gastos">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                 </children>
                              </VBox>
                           </content>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" contentDisplay="TOP" layoutX="10.0" layoutY="266.0" text="Incidentes">
                           <graphic>
                              <ImageView id="imvIncidentes" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                           <content>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" minWidth="208.0" onAction="#accionCategorias" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Categorias">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" minWidth="208.0" onAction="#accionRegistrarIncidente" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Registro de Incidentes">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" minWidth="50.0" onAction="#accionSeguimientoIncidentes" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Seguimiento de Incidentes">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                 </children>
                              </VBox>
                           </content>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" contentDisplay="TOP" layoutX="10.0" layoutY="291.0" text="Reportes">
                           <content>
                              <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionReporteGastos" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Gastos de Servicios">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionReporteIncidentes" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Incidentes Registrados">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                    <JFXButton maxHeight="50.0" minHeight="50.0" onAction="#accionHoraLaboradas" prefHeight="50.0" prefWidth="260.0" styleClass="button_3" text="Horas Laboradas">
                                       <VBox.margin>
                                          <Insets bottom="2.5" top="2.5" />
                                       </VBox.margin>
                                    </JFXButton>
                                 </children>
                              </VBox>
                           </content>
                           <graphic>
                              <ImageView id="imvReport" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="TOP" expanded="false" layoutX="10.0" layoutY="316.0" text="Transacciones">
                           <graphic>
                              <ImageView id="imvTrasaccion" fitHeight="38.0" fitWidth="38.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                        </TitledPane>
                        <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="TOP" expanded="false" layoutX="10.0" layoutY="518.0" text="Parametros del Sistema">
                           <graphic>
                              <ImageView id="imvParametros" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" smooth="false" />
                           </graphic>
                        </TitledPane>
                     </panes>
                  </Accordion>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </left>
   <center>
      <VBox fx:id="vbContenedor" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
